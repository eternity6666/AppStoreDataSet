  "platformAttributes.ios.editorialNotes.standard": [
    {
      "k": "The original Block Fortress was an innovative blend of Minecraft-inspired building and first-person tower-defense mechanics. With Block Fortress: War, you can take the fight online in action-packed battles or make your way through a huge, planet-spanning campaign. The intense combat requires tactical thinking, troop management, and resource collection. Returning fans will feel right at home constructing impenetrable fortresses, while franchise newcomers will love mastering this elaborate game.",
      "v": {
        "ipad": {
          "us": [
            "2025-03-24",
            "2025-03-30"
          ]
        }
